{
  "name": "nest-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:typings": "ts-node ./src/generate-typings",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ross-env NODE_ENV=development nest nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "ross-env NODE_ENV=development nest nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.11",
    "@nestjs/config": "^0.4.2",
    "@nestjs/core": "^7.0.11",
    "@nestjs/graphql": "^7.3.9",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^7.0.13",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.11",
    "@nestjs/platform-socket.io": "^7.0.11",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/websockets": "^7.0.11",
    "apollo-server-express": "^2.13.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "graphql": "^15.0.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^5.0.0",
    "hbs": "^4.1.1",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.14",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "socket.io-redis": "^5.2.0",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.11",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.0",
    "@types/jest": "25.2.2",
    "@types/mongoose": "^5.7.19",
    "@types/node": "^14.0.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.5.1",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
